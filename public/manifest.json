{
  "name": "Primitive Scrape",
  "description": "An extension to scrape data off of websites that you visit as a normal person",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["storage", "activeTab", "scripting", "unlimitedStorage"],
  "background": {
    "service_worker": "worker.js"
  },
  "options_page": "options_page.html",
  "icons": {
    "16": "Icon-16.png",
    "48": "Icon-48.png",
    "128": "Icon-128.png"
  }
}